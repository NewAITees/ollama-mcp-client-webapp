{
    "mcpServers": {
      "TodoApplication": {
        "command": "wsl.exe",
        "args": [
          "-e", 
          "bash", 
          "-c", 
          "cd /home/persona/analysis/TaskMateAI/src/TaskMateAI && /home/persona/.local/bin/uv run TaskMateAI"
        ],
        "env": {},
        "alwaysAllow": [
          "get_tasks", "get_next_task", "create_task", "update_progress", 
                 "complete_task", "add_subtask", "update_subtask"
        ],
        "defaultArguments": {
          "agent_id": "agent_123",
          "project_name": ""
        } 
      },
      "crawl4ai-mcp-server": {
        "command": "python",
        "args": ["C:/analysis/crawl4ai-mcp-server/src/index.py"],
        "env": { 
          "GOOGLE_API_KEY": "config.googleApiKey", 
          "GOOGLE_CSE_ID": "config.googleCseId"
        }
      },
      "obsidian": {
        "command": "node",
        "args": ["C:\\analysis\\obsidian-mcp\\build\\main.js", "C:\\obsidian\\VolcanicAsh"]
      },
      "ollama-MCP-server": {
        "command": "wsl.exe",
        "args": [
          "-e",
          "bash",
          "-c",
          "cd /home/persona/analysis/ollama-MCP-server && /home/persona/.local/bin/uv run ollama-MCP-server"
        ],
        "env": {
          "model": "deepseek-r1:14b"
        }
      }
    }
}